<%- include('../layouts/adminHeader.ejs') %>
<!-- partial -->
<div class="main-panel">
<div class="content-wrapper">
<div class="page-header">
<h3 class="page-title">Coupon Management</h3>
<nav aria-label="breadcrumb">
<a href="/admin/addCoupon" class="btn btn-primary btn-fw">Add Coupon</a>
</nav>
</div>
<div class="row">
<div class="col-lg-12 grid-margin stretch-card">
<div class="card">
  <div class="card-body">
    <h4 class="card-title">Coupon List</h4>
    
    <div class="table-responsive">
      <table class="table table-dark">
        <thead>
          <tr>
            <th>Coupon Code</th>
            <th>Discount</th>
            <th>Min Purchase</th>
            <th>Max Discount Value</th>
            <th>Validity</th>
          </tr>
        </thead>
        <tbody>
            <% couponList.forEach((coupon) => { %>
            <tr>
                <td><%= coupon.couponCode %></td>
                <td><%= coupon.minDiscountPercentage %> %</td>
                <td>₹<%= coupon.minPurchase %></td>
                <td>₹<%= coupon.maxDiscountValue %></td>
                <td><%= coupon.description %></td>
            </tr>
            <% }); %>
        </tbody>
      </table>
    </div>
  </div>
</div>
</div>
</div>
</div>
</div>
  <!-- content-wrapper ends -->
  <!-- partial:../../partials/_footer.html -->
<%- include('../layouts/adminFooter.ejs') %>